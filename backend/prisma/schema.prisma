generator client {
  provider = "prisma-client-js"
}
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Registration {
  id               Int      @id @default(autoincrement())
  aadhaarNumber    String
  entrepreneurName String
  orgType          String
  panNumber        String
  panHolderName    String
  dobOrDoi         DateTime
  createdAt        DateTime @default(now())
}


